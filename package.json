{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Just a basic React Webpack boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "start": "webpack-dev-server --config webpack.dev.js",
    "lint": "eslint . --ext .js --ext .jsx --cache",
    "sass-lint": "sass-lint -c .scss-lint.yml ./app/components/**/*.scss -v -q",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bimah/react-boilerplate.git"
  },
  "pre-commit": [
    "lint",
    "sass-lint",
    "build"
  ],
  "author": "Matteo Corazzin",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.5",
    "identity-obj-proxy": "^3.0.0",
    "install": "^0.10.1",
    "jest": "^22.4.2",
    "jest-fetch-mock": "^1.3.3",
    "node-sass": "^4.8.1",
    "postcss-loader": "^2.0.8",
    "precommit-hook-eslint": "^3.0.0",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.1.1",
    "rtl-css-loader": "^1.0.1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "jest": {
    "testRegex": "(.*).test.js$",
    "moduleNameMapper": {
      "\\.(scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./app/__tests__/setup.js"
    ]
  }
}
